<table>
    <h1>Songs List</h1>
    <thead>
        <tr>
            <td><b>Song</b></td>
            <td><b>Delete?</b></td>
        </tr>
    </thead>
    <% @songs.each do |song| %>
        <tbody>
            <div class="song">
                <tr>
                    <span><%= link_to song.title, song_path(song) %></span>
                    <%= link_to "Delete", song, method: :delete, data: { confirm: "Really?" } %>
                </tr>
            </div>
        </tbody>
    <% end %>
</table>